{
  "root": true,
  "env": {
    "node": true
  },
  // "extends": "airbnb-base",
  "extends": ["plugin:vue/essential", "@vue/standard"],
  "rules": {
    "no-console":"error",
    "no-debugger":"off",
    "eqeqeq": 0,
    "no-new": 0,
    "no-callback-literal": 0
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}